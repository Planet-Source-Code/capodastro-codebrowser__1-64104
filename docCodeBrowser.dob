316     HookMainWindow
318     'If ProcessMsg = False Then ProcessMsg = True
320 Case 13
        'disable hooks
322     UnhookMainWindow
324     SaveSetting App.Title, "Settings", "Hook", "Disabled"
    End Select
    Exit Sub
eH:
326 Select Case Err.Number
    Case 35605 'Das Steuerelement dieses Elementes wurde geloescht
